{
    "name": "hiatusss/custompost-command",
    "description": "Custom post can use external files",
    "type": "wp-cli-package",
    "homepage": "https://github.com/hiatusss/custompost-command",
    "support": {
        "issues": "https://github.com/hiatusss/custompost-command/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "custompost-command.php"
        ]
    },
    "require": {
        "wp-cli/wp-cli": "*"
    },
    "require-dev": {
        
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "commands": [
            "custompost",
            "custompost create"
        ],
        "bundled": true
    }
}
